{
  "name": "@agntk/client",
  "version": "0.1.2",
  "type": "module",
  "description": "HTTP/WebSocket client for @agntk/server",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc && node ../../scripts/fix-esm-imports.mjs ./dist",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agntk/logger": "workspace:*",
    "ai": "6.0.77"
  },
  "devDependencies": {
    "@agntk/config-typescript": "workspace:*",
    "typescript": "^5.7.3",
    "vitest": "^3.2.3",
    "@types/node": "^20.17.19"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/agntk/agntk.git",
    "directory": "packages/sdk-client"
  },
  "homepage": "https://github.com/agntk/agntk#readme",
  "bugs": {
    "url": "https://github.com/agntk/agntk/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "ai",
    "agent",
    "client",
    "http",
    "websocket",
    "sse"
  ],
  "license": "MIT"
}
