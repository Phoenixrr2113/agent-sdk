{
  "name": "@agent/sdk-client",
  "version": "0.1.0",
  "type": "module",
  "description": "HTTP/WebSocket client for @agent/sdk-server",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agent/logger": "workspace:*",
    "ai": "6.0.77"
  },
  "devDependencies": {
    "@agent/config-typescript": "workspace:*",
    "typescript": "^5.7.3",
    "vitest": "^1.6.1",
    "@types/node": "^20.17.19"
  },
  "keywords": ["ai", "agent", "client", "http", "websocket"],
  "license": "MIT"
}
